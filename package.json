{
  "name": "@dotted-labs/ngx-signal-utils",
  "version": "0.0.5",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:ngx-utils && npm run build:ngx-signal-utils && npm run build:ngx-bootstrap-components",
    "build:ngx-utils": "ng build ngx-utils && cp *.md dist/ngx-utils/",
    "build:ngx-signal-utils": "ng build ngx-signal-utils && cp *.md dist/ngx-signal-utils/",
    "build:ngx-bootstrap-components": "ng build ngx-bootstrap-components && cp *.md dist/ngx-bootstrap-components/",
    "publish": "npm run build && npm run publish:ngx-utils && npm run publish:ngx-signal-utils && npm run publish:ngx-bootstrap-components",
    "publish:ngx-utils": " cd dist/ngx-utils && npm publish --access public",
    "publish:ngx-signal-utils": " cd dist/ngx-signal-utils && npm publish --access public",
    "publish:ngx-bootstrap-components": " cd dist/ngx-bootstrap-components && npm publish --access public",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint ngx-signal-utils --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.2.4",
    "@angular/common": "19.2.4",
    "@angular/compiler": "19.2.4",
    "@angular/core": "19.2.4",
    "@angular/forms": "19.2.4",
    "@angular/platform-browser": "19.2.4",
    "@angular/platform-browser-dynamic": "19.2.4",
    "@angular/router": "19.2.4",
    "@ngrx/signals": "^19.0.1",
    "@ngx-translate/core": ">=15.0.0",
    "rxjs": "~7.8.0",
    "tslib": ">=2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.5",
    "@angular-eslint/builder": ">=18.0.0",
    "@angular-eslint/eslint-plugin": ">=18.0.0",
    "@angular-eslint/eslint-plugin-template": ">=18.0.0",
    "@angular-eslint/schematics": ">=18.0.0",
    "@angular-eslint/template-parser": ">=18.0.0",
    "@angular/cli": "19.2.5",
    "@angular/compiler-cli": "19.2.4",
    "@ngrx/eslint-plugin": ">=18.0.0",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "eslint": ">=8.57.0",
    "eslint-config-prettier": ">=9.1.0",
    "eslint-plugin-import": ">=2.29.1",
    "eslint-plugin-prettier": ">=5.1.3",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "19.2.0",
    "prettier": ">=3.2.5",
    "typescript": "~5.8.2",
    "typescript-eslint": ">=7.8.0"
  }
}
